{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_SECRET_KEY": "@next_public_secret_key",
    "NEXT_PUBLIC_YT_API": "@next_public_yt_api",
    "NEXT_PUBLIC_PASSWORD_USER": "@next_public_password_user"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
